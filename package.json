    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "clean": "rm -rf node_modules package-lock.json yarn.lock",
    "lint:eslint": "eslint --max-warnings 0  \"src/**/*.{ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx}\"",
    "lint:stylelint": "stylelint --max-warnings 0  \"src/**/*.{css,scss}\" --fix",
    "format": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "prepare": "husky install",
    "test": "jest --watch",
    "test:ci": "jest --ci"
  },
